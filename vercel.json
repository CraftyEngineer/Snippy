{
    "version": 2,
    "builds": [
      {
        "src": "package.json",
        "use": "@vercel/static-build",
        "config": {
          "distDir": "dist",
          "installCommand": "npm install",
          "buildCommand": "npm run build"
        }
      },
      {
        "src": "api/app.py",
        "use": "@vercel/python",
        "config": {
            "runtime": "python3.9",
            "installCommand": "pip install -r requirements.txt --target ."
        }
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "api/app.py"
      },
      {
        "src": "/(.*)",
        "dest": "dist/$1"
      }
    ]
  }